				{
					"name": "Step 1: Create a HTTP Monitor",
					"request": {
						"url": "https://{{bigip_mgmt}}/mgmt/tm/ltm/monitor/http",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "X-F5-Auth-Token",
								"value": "{{bigip_token}}",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"name\":\"module_3_http_monitor\",\n    \"send\":\"GET / HTTP/1.0\\r\\n\\r\\n\",\n    \"recv\":\"\"\n}"
						},
						"description": ""
					},
					"response": []
				},
				{
